type Query {
  getTransactionsForImage(imageId: String!): [Transaction]!
}

type Mutation {
  validateTransaction(sender: String!, receiver: String!, imageId: String!): TransactionResult!
}

type TransactionResult {
  success: Boolean!
  result: Int!
}

type Transaction {
  sender: String!
  receiver: String!
  imageId: String!
}