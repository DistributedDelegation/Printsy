type Query {
  checkImageTransactionCount(imageId: Float!): CountResult!
}

type Mutation {
  completeTransaction(transactions: [TransactionInput!]!): TransactionResult!
}

input TransactionInput {
  userId: Float!
  productId: Float!
  imageId: Float!
}

type Transaction {
  userId: Float!
  productId: Float!
  imageId: Float!
}

type CountResult {
  count: Int!
}

type TransactionResult {
  success: Boolean!
}
